<%= form_for @user do |f| %>

  <%= f.label :first_name %>:
  <%= f.text_field :first_name, class: 'form-control' %><br />

  <%= f.label :last_name %>:
  <%= f.text_field :last_name, class: 'form-control' %><br />

  <%= f.label :country_code %>:
  <%= f.text_field :country_code, class: 'form-control', id: 'authy-countries' %><br />

  <%= f.label :phone_number %>:
  <%= f.text_field :phone_number, class: 'form-control', id: 'authy-cellphone' %><br />

  <%= f.label :email %>:
  <%= f.text_field :email, class: 'form-control' %><br />

  <%= f.label :password %>:
  <%= f.text_field :password, class: 'form-control' %><br />


  <%= f.submit 'Next >' %>

<% end %>